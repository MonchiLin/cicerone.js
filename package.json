{
  "name": "cicerone.js",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite build --watch & tsc -w",
    "build": "vite build & tsc",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "18.17.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-dts": "^3.4.0",
    "vitest": "^0.33.0"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./bs-popover": {
      "import": "./dist/bs-popover.js",
      "require": "./dist/bs-popover.unm.js",
      "types": "./dist/bs-popover.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/bs-popover/style.css": {
      "import": "./dist/bs-popover/style.css",
      "require": "./dist/bs-popover/style.css"
    }
  },
  "files": [
    "dist/**/*"
  ]
}
